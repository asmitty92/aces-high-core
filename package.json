{
  "name": "aces-high-core",
  "version": "2.0.0",
  "description": "Base framework for card and deck functionality",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "build": "jsii",
    "build:watch": "jsii --watch",
    "package": "jsii-pacmak",
    "deploy:pypi": "publib-pypi",
    "deploy:npm": "publib-npm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubyclimber/aces-high-core.git"
  },
  "author": "Aaron Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rubyclimber/aces-high-core/issues"
  },
  "homepage": "https://github.com/rubyclimber/aces-high-core#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "jest": "^27.4.7",
    "jsii": "^5.7.4",
    "jsii-pacmak": "^1.106.0",
    "publib": "^0.2.938",
    "ts-jest": "^27.1.3"
  },
  "stability": "stable",
  "jsii": {
    "versionFormat": "full",
    "tsconfig": "tsconfig.json",
    "validateTsconfig": "minimal",
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "aces-high-core",
        "module": "aces_high_core"
      }
    }
  }
}
